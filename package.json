{
  "name": "climbmapper",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "bcrypt-pbkdf": "^1.0.1",
    "body-parser": "~1.13.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "^4.13.4",
    "express-dynamic-helpers-patch": "0.0.2",
    "express-flash": "0.0.2",
    "express-session": "^1.14.2",
    "hjs": "~0.0.6",
    "hogan-express": "^0.5.2",
    "jasmine-node": "^1.14.5",
    "jquery": "^2.2.4",
    "jsdom": "^7.2.2",
    "leaflet": "^1.0.2",
    "morgan": "~1.6.1",
    "passport": "^0.2.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "python-shell": "^0.3.0",
    "react": "^15.4.0",
    "react-d3-library": "^1.1.7",
    "react-dom": "^15.4.0",
    "react-leaflet": "^1.0.2",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "request": "^2.79.0",
    "serve-favicon": "^2.3.2",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "enzyme": "^2.7.0",
    "jest": "^18.1.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "redux-devtools": "^3.3.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.14.0"
  }
}
